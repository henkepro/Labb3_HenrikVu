<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="View\Icons\correct.png" />
    <None Remove="View\Icons\incorrect.png" />
    <None Remove="View\Icons\letter-q.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="FontAwesome.Sharp" Version="6.6.0" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="View\Icons\correct.png" />
    <Resource Include="View\Icons\incorrect.png" />
    <Resource Include="View\Icons\letter-q.ico" />
  </ItemGroup>

</Project>
